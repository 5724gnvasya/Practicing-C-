#include <iostream>

using namespace std;

int main()
{
    int y;
    cin>>y;
    if ((y%4==0 and y%100!=0) or y%400==0)
    {
        if ( y>=100 and y <=999)
        {
            cout<<"12/09/0"<<y;
        }
        if ( y>=10 and y <=99)
        {
            cout<<"12/09/00"<<y;
        }
        if ( y>=1 and y <=9)
        {
            cout<<"12/09/000"<<y;
        }
      if ( y>=1000 )
        {
            cout<<"12/09/"<<y;
        }
    }
    else
    {
        if ( y>=100 and y <=999)
        {
            cout<<"13/09/0"<<y;
        }
        if ( y>=10 and y <=99)
        {
            cout<<"13/09/00"<<y;
        }
        if ( y>=1 and y <=9)
        {
            cout<<"13/09/000"<<y;
        }
       if ( y>=1000 )
        {
            cout<<"13/09/"<<y;
        }
    }
    return 0;
}
